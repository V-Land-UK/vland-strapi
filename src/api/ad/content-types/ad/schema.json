{
  "kind": "collectionType",
  "collectionName": "ads",
  "info": {
    "singularName": "ad",
    "pluralName": "ads",
    "displayName": "Ad",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "expiry": {
      "type": "datetime"
    },
    "name": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "small",
        "medium",
        "large"
      ],
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "ads"
    }
  }
}
